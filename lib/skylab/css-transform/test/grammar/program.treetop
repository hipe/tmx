module Hipe
  module CssConvert
    module Grammar
      grammar Program
        include Common
        rule itt_phrase
          [Ii] "n" (space "the")? space "folder" space folder:quoted_string_or_word end_of_phrase {
            def tree
              [:in_the_folder, {:the_word => folder.value} ]
            end
          }
        end
      end
    end
  end
end
